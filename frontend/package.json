{
  "name": "green-ecolution-frontend",
  "private": true,
  "version": "1.2.1",
  "license": "AGPL-3.0-only",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "pnpm build && pnpm preview",
    "dev": "pnpm --filter frontend dev",
    "dev:remote": "VITE_BACKEND_BASEURL=/api-stage pnpm --filter frontend dev",
    "build": "pnpm --filter=!frontend -r build && pnpm --filter frontend build",
    "build:stage": "pnpm --filter=!frontend -r build && pnpm --filter frontend build:stage",
    "build:backend-client": "pnpm --filter backend-client build",
    "build:plugin-interface": "pnpm --filter plugin-interface build",
    "generate": "pnpm --filter backend-client generate",
    "generate:stage": "pnpm --filter backend-client generate:stage",
    "generate:local": "pnpm --filter backend-client generate:local",
    "ci": "pnpm --filter backend-client generate:ci",
    "lint": "prettier --check . && pnpm --filter frontend lint",
    "test": "echo \"It works!\"",
    "preview": "pnpm --filter frontend preview",
    "fmt": "prettier --write ."
  },
  "devDependencies": {
    "prettier": "^3.6.2"
  },
  "packageManager": "pnpm@10.20.0"
}
